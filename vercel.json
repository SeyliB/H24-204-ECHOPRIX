{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "server/python_scripts/timer.py",
        "use": "@vercel/python"
      },
      {
        "src": "server/python_scripts/categorizer.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
          "src": "/(.*)", 
          "dest": "/server/routes/$1" 
        },
        {
          "src": "/(.*)", 
          "dest": "/index.js" 
        }
    ]
  }